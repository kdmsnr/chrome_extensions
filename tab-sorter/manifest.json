{
  "manifest_version": 3,
  "name": "Tab Sorter",
  "version": "1.0",
  "description": "Sort tabs",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_title": "Sort Tabs",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
